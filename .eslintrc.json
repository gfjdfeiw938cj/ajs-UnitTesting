{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": ["error", "LabeledStatement", "WithStatement"],
    "linebreak-style": 0,
    "no-console": ["error", { "allow": ["warn", "error", "log"] }],
    "no-unused-vars": "off",
    "no-new": 0,
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 0,
    "no-nested-ternary": "off",
    "keyword-spacing": "off"
  }
}